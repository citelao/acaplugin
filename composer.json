{
    "name": "citelao/acac-features",
    "description": "ACAC features plugin :)",
    "authors": [
        {
            "name": "Ben",
            "email": "ben@stolovitz.com"
        }
    ],
    "repositories": [
        {
          "type": "composer",
          "url": "https://wpackagist.org"
      }
    ],
    "require": {
        "wpackagist-plugin/cmb2": "2.2.*"
    },
    "autoload": {
        "files": [
          "vendor/cmb2/init.php"
        ],
        "psr-4": {
            "Acaplugin\\": "src/"
        }
    },
    "extra": {
        "installer-paths": {
            "vendor/{$name}/": ["type:wordpress-plugin", "type:wordpress-theme"]
        }
    }
}
